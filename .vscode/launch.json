    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: VIO w/ args",
            "type": "debugpy",
            "request": "launch",
            "program": "Code/vio.py",
            "console": "integratedTerminal",
            "args": "--view --path data"
        },
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        }
    ]
}